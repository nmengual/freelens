{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "static/build/**"]
    },
    "build:app": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "static/build/**"]
    },
    "rebuild": {
      "dependsOn": ["^rebuild"],
      "outputs": ["dist/**", "static/build/**"]
    },
    "clean": {
      "cache": false
    },
    "lint": {},
    "lint:fix": {
      "cache": false
    },
    "test:unit": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test:integration": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "compute-versions": {
      "cache": false
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:main": {
      "cache": false,
      "persistent": true
    },
    "dev:renderer": {
      "cache": false,
      "persistent": true
    }
  }
}
